<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUser</servlet-name>
        <servlet-class>Servlets.GetUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AltaPregunta</servlet-name>
        <servlet-class>Servlets.AltaPregunta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarPregunta</servlet-name>
        <servlet-class>Servlets.ModificaPreguntas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearExamen</servlet-name>
        <servlet-class>Servlets.CrearExamen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IrExamen</servlet-name>
        <servlet-class>Servlets.IrExamen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IrPreguntas</servlet-name>
        <servlet-class>Servlets.IrPreguntas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ObtenerPreguntas</servlet-name>
        <servlet-class>Servlets.ObtenerPreguntas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerPreguntas</servlet-name>
        <servlet-class>Servlets.VerPreguntas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUser</servlet-name>
        <url-pattern>/GetUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AltaPregunta</servlet-name>
        <url-pattern>/AltaPregunta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarPregunta</servlet-name>
        <url-pattern>/ModificarPregunta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearExamen</servlet-name>
        <url-pattern>/CrearExamen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IrExamen</servlet-name>
        <url-pattern>/IrExamen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IrPreguntas</servlet-name>
        <url-pattern>/IrPreguntas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObtenerPreguntas</servlet-name>
        <url-pattern>/ObtenerPreguntas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerPreguntas</servlet-name>
        <url-pattern>/VerPreguntas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
